<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
  targetNamespace="urn:vpro:media:2009" xmlns="urn:vpro:media:2009"
  xmlns:shared="urn:vpro:shared:2009" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:import namespace="urn:vpro:shared:2009" schemaLocation="vproShared.xsd"/>

  <xs:annotation>
    <xs:documentation>This is the XSD for the POMS domain.</xs:documentation>
  </xs:annotation>

  <xs:element name="mediaInformation" type="mediaTableType">
    <xs:annotation>
      <xs:documentation>
        Base element only used when programs, groups and schedule information need to be bundled in one XML. E.g. when distributing to cable companies.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="program" type="programType">
    <xs:annotation>
      <xs:documentation>
        This is the most used entity in POMs. It represent e.g. one broadcast program or one webonly clip. It represent a standalone entity which a consumer can view or listen to.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="group" type="groupType">
    <xs:annotation>
      <xs:documentation>
        A groups collects a number of programs and/or other groups. E.g. seasons, series, playlists and albums..
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="segment" type="segmentType">
    <xs:annotation>
      <xs:documentation>
        Any program can contain a number of 'segments'. A segment is an identifiable part of a program.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="schedule" type="scheduleType">
    <xs:annotation>
      <xs:documentation>
        Programs (of the type 'BROADCAST') can contain 'schedule events' which indicate when and on which channels it is or was broadcast. A schedule is a container which only contains the 'schedule event' of different programs, but collected for a certain period of time.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="mediaTableType">
    <xs:sequence>
      <xs:element type="programTableType" name="programTable" minOccurs="0">
        <xs:annotation>
          <xs:documentation> A table with all program object in this container</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="groupTableType" name="groupTable" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A table with all group object in this container</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="locationTableType" name="locationTable" minOccurs="0"/>
      <xs:element ref="schedule" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A table with all schedule information in this containe
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute type="xs:string" name="publisher"/>
    <xs:attribute type="xs:dateTime" name="publicationTime"/>
    <xs:attribute type="xs:short" name="version"/>
  </xs:complexType>

  <xs:complexType name="programTableType">
    <xs:sequence>
      <xs:element ref="program" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="programType">
    <xs:complexContent>
      <xs:extension base="baseMediaType">
        <xs:sequence>
          <xs:element name="episodeOf" type="memberRefType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>
                A program (only if its type is 'BROADCAST') can be an episode of a group of type 'SERIES' or 'SEASON'.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="segments" type="segmentsType" minOccurs="0"/>
          <!--MSE-2330
          <xs:element name="poSeriesID" type="xs:string" minOccurs="0"/>
          <xs:element name="poProgType" type="xs:string" minOccurs="0"/>
          <xs:element name="poMiscInfo" type="xs:string" minOccurs="0"/>
          <xs:element name="poSeasonSubTitle" type="xs:string" minOccurs="0"/>
          -->
        </xs:sequence>
        <xs:attribute type="programTypeEnum" name="type" use="required">
          <xs:annotation>
            <xs:documentation>
              The type of this program (e.g. BROADCAST, TRACK, CLIP)
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:simpleType name="mediaTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MEDIA"/>
      <xs:enumeration value="STRAND"/>
      <xs:enumeration value="ALBUM"/>
      <xs:enumeration value="PLAYLIST"/>
      <xs:enumeration value="ARCHIVE"/>
      <xs:enumeration value="UG ARCHIVE"/>
      <xs:enumeration value="SEASON"/>
      <xs:enumeration value="SERIES"/>
      <xs:enumeration value="BROADCAST"/>
      <xs:enumeration value="MOVIE"/>
      <xs:enumeration value="TRAILER"/>
      <xs:enumeration value="CLIP"/>
      <xs:enumeration value="STRAND"/>
      <xs:enumeration value="TRACK"/>
      <xs:enumeration value="SEGMENT"/>
      <xs:enumeration value="VISUALRADIO"/>
      <xs:enumeration value="PROMO"/>
      <xs:enumeration value="RECORDING"/>
      <xs:enumeration value="COLLECTION"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="groupTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="STRAND"/>
      <xs:enumeration value="ALBUM"/>
      <xs:enumeration value="PLAYLIST"/>
      <xs:enumeration value="ARCHIVE"/>
      <xs:enumeration value="COLLECTION"/>
      <xs:enumeration value="SEASON"/>
      <xs:enumeration value="SERIES"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="programTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="BROADCAST"/>
      <xs:enumeration value="MOVIE"/>
      <xs:enumeration value="TRAILER"/>
      <xs:enumeration value="CLIP"/>
      <xs:enumeration value="STRAND"/>
      <xs:enumeration value="TRACK"/>
      <xs:enumeration value="VISUALRADIO"/>
      <xs:enumeration value="PROMO"/>
      <xs:enumeration value="RECORDING"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="segmentTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SEGMENT"/>
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="workflowTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PUBLISHED"/>
      <xs:enumeration value="REVOKED"/>
      <xs:enumeration value="FOR REPUBLICATION"/>
      <xs:enumeration value="FOR PUBLICATION"/>
      <xs:enumeration value="DELETED"/>
      <xs:enumeration value="PARENT REVOKED"/>
    </xs:restriction>
  </xs:simpleType>


  <xs:complexType name="baseMediaType" abstract="true">
    <xs:annotation>
      <xs:documentation>
        This is the abstract base entity for program's, group's and segment. Actually these object are very similar and share most properties.
        </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element type="xs:anyURI" name="crid" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Crids are used to refer to entities in other systems. E.g. a crid like
            crid://broadcast.radiobox2/335793  woudl refer to a broadcast with id 335793 in
            Radiobox. A crid must be a valid URI  starting with "crid://". The must be unique, but they can be made up freely.
            Het is a good idea to use a logic structure which can easily be associated with another system.
            Any POMS object can have zero or more CRIDS. This can refer to different systems, or it can mean that the POMS object actually represents more than one entity in the remote system.
             </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="organizationType" name="broadcaster" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>One or more broadcaster can be the owner of a POMS media object. This information is meta information about the object, but it is also used
            for assigning write access to the object in the POMS backend to employees if these given broadcasting companies.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="organizationType" name="portal" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Optionally also 'portals' can be assigned to a media object. Portals are also 'owners', and employees can also work for a certain portal.
            This is because some portal are shared by several broadcasting companies.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="portalRestrictionType" name="exclusive" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Besides having portals, which mainly indicates where the object originates, a media object can also be assigned 'portal restrictions'.
            If it does, that means that the media object may not be used in other portals at all.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="geoRestrictionType" name="region" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Media with a geo restrictie can only be played (!) in the indicated geo region
            (NL, BENELUX, WORLD)</xs:documentation>
          <xs:documentation>
            This doesn't apply to the meta data of the media object. It only applies to viewing the actual playable content.
            </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="titleType" name="title" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>One or more titles</xs:documentation>
          <xs:documentation>Titels hebben altijd een type (MAIN, SUB etc.) en een owner
            (BROADCASTER, MIS etc.) de combinatie type-owner is altijd uniek binnen één media
            instantie; er zijn dus nooit twee titels met zelfde type en owner binnen één
            programma.</xs:documentation>
          <xs:documentation>Titles worden bij publicatie eerst op volgorde van de textualTypeEnum en
            daarbinnen op volgorde van de ownerTypeEnum geordend. Eerste titel in een gepubliceerd
            document is dus titel met BROADCASTER - MAIN, mist deze beschikbaar
            is.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="descriptionType" name="description" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Optionele eschrijvingen bij deze media</xs:documentation>
          <xs:documentation>Beschrijvingen krijgen dezelfde verplichte attributen en hiervoor gelden
            dezelfde regels.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="genreType" name="genre" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="tagType" name="tag" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="xs:string" name="source" minOccurs="0"/>
      <xs:element type="countryType" name="country" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="languageType" name="language" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="avAttributesType" name="avAttributes" minOccurs="0"/>
      <xs:element type="xs:short" name="releaseDate" minOccurs="0"/>
      <xs:element type="xs:duration" name="duration" minOccurs="0"/>
      <xs:element type="creditsType" name="credits" minOccurs="0"/>
      <xs:element type="xs:string" name="award" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="descendantRefType" name="descendantOf" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="memberRefType" name="memberOf" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="ageRatingType" name="ageRating" minOccurs="0"/>
      <xs:element type="contentRatingType" name="contentRating" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="xs:anyURI" name="email" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="websiteType" name="website" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="twitterType" name="twitter" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="xs:short" name="teletext" minOccurs="0" maxOccurs="1"/>
      <xs:element type="predictionType" name="prediction" minOccurs="0" maxOccurs="2"/>
      <xs:element type="locationsType" name="locations" minOccurs="0"/>
      <xs:element type="scheduleEventsType" name="scheduleEvents" minOccurs="0"/>
      <xs:element type="relationType" name="relation" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="imagesType" name="images" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="midType" name="mid"/>
    <!--TODO required when all media has a mid-->
    <xs:attribute type="avTypeEnum" name="avType" use="required"/>
    <xs:attribute type="xs:dateTime" name="sortDate" use="optional"/>
    <xs:attribute type="xs:boolean" name="embeddable" use="optional" default="true"/>
    <xs:attribute type="xs:boolean" name="hasSubtitles" use="optional" default="false"/>
    <xs:attributeGroup ref="shared:publishableAttributesGroup"/>
    <xs:attribute type="midType" name="mergedTo" use="optional"/>
  </xs:complexType>

  <xs:attributeGroup name="restrictionAttributesGroup">
    <xs:attribute name="start" type="xs:dateTime"/>
    <xs:attribute name="stop" type="xs:dateTime"/>
  </xs:attributeGroup>

  <xs:complexType name="portalRestrictionType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:string" name="portalId"/>
        <xs:attributeGroup ref="restrictionAttributesGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="geoRestrictionType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="geoRestrictionEnum" name="regionId"/>
        <xs:attributeGroup ref="restrictionAttributesGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="cridType">
    <xs:restriction base="xs:anyURI">
      <xs:pattern value="(c|C)(r|R)(i|I)(d|D)://.*/.*"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="platformTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="INTERNETVOD"/>
      <xs:enumeration value="TVVOD"/>
      <xs:enumeration value="PLUSVOD"/>
      <xs:enumeration value="NPOPLUSVOD"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="titleType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="textualAttributes"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="tagType">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:complexType name="descriptionType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="textualAttributes"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:attributeGroup name="textualAttributes">
    <xs:attribute type="textualTypeEnum" name="type"/>
    <xs:attribute type="shared:ownerTypeEnum" name="owner" use="required"/>
  </xs:attributeGroup>

  <xs:simpleType name="textualTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MAIN"/>
      <xs:enumeration value="SHORT"/>
      <xs:enumeration value="SUB"/>
      <xs:enumeration value="KICKER"/>
      <xs:enumeration value="ORIGINAL"/>
      <xs:enumeration value="EPISODE"/>
      <xs:enumeration value="WORK"/>
      <xs:enumeration value="LEXICO"/>
      <xs:enumeration value="ABBREVIATION"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="avTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AUDIO"/>
      <xs:enumeration value="VIDEO"/>
      <xs:enumeration value="MIXED"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="broadcasterType">
    <xs:sequence>
      <xs:element type="organizationType" name="broadcaster" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="organizationType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="organizationIdType" name="id"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="portalsType">
    <xs:sequence>
      <xs:element type="organizationType" name="portal" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="repeatType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:boolean" name="isRerun" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="avAttributesType">
    <xs:sequence>
      <xs:element type="xs:long" name="bitrate" minOccurs="0"/>
      <xs:element type="avFileFormatEnum" name="avFileFormat" minOccurs="0"/>
      <xs:element type="videoAttributesType" name="videoAttributes" minOccurs="0"/>
      <xs:element type="audioAttributesType" name="audioAttributes" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="avFileFormatEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MP3"/>
      <xs:enumeration value="RA"/>
      <xs:enumeration value="RM"/>
      <xs:enumeration value="MP4"/>
      <xs:enumeration value="WVC1"/>
      <xs:enumeration value="WM"/>
      <xs:enumeration value="RAM"/>
      <xs:enumeration value="WMP"/>
      <xs:enumeration value="HTML"/>
      <xs:enumeration value="M4A"/>
      <xs:enumeration value="M4V"/>
      <xs:enumeration value="DGPP"/>
      <xs:enumeration value="FLV"/>
      <xs:enumeration value="HASP"/>
      <xs:enumeration value="MPEG2"/>
      <xs:enumeration value="H264"/>
      <xs:enumeration value="UNKNOWN"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="videoAttributesType">
    <xs:sequence>
      <xs:element type="colorType" name="color" minOccurs="0"/>
      <xs:element type="xs:string" name="videoCoding" minOccurs="0"/>
      <xs:element type="aspectRatioEnum" name="aspectRatio" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:short" name="height"/>
    <xs:attribute type="xs:short" name="width"/>
  </xs:complexType>

  <xs:simpleType name="colorType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="COLOR"/>
      <xs:enumeration value="BLACK AND WHITE"/>
      <xs:enumeration value="BLACK AND WHITE AND COLOR"/>
      <xs:enumeration value="COLORIZED"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="aspectRatioEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="4:3"/>
      <xs:enumeration value="16:9"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="audioAttributesType">
    <xs:sequence>
      <xs:element type="xs:short" name="numberOfChannels" minOccurs="0"/>
      <xs:element type="xs:string" name="audioCoding" minOccurs="0"/>
      <xs:element type="xs:string" name="language" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="creditsType">
    <xs:sequence>
      <xs:element type="personType" name="person" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="personType">
    <xs:sequence>
      <xs:element type="xs:string" name="givenName"/>
      <xs:element type="xs:string" name="familyName"/>
    </xs:sequence>
    <xs:attribute type="roleType" name="role" use="required"/>
  </xs:complexType>

  <xs:simpleType name="roleType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="DIRECTOR"/>
      <xs:enumeration value="CHIEF_EDITOR"/>
      <xs:enumeration value="EDITOR"/>
      <xs:enumeration value="PRESENTER"/>
      <xs:enumeration value="INTERVIEWER"/>
      <xs:enumeration value="PRODUCER"/>
      <xs:enumeration value="RESEARCH"/>
      <xs:enumeration value="GUEST"/>
      <xs:enumeration value="REPORTER"/>
      <xs:enumeration value="ACTOR"/>
      <xs:enumeration value="COMMENTATOR"/>
      <xs:enumeration value="SCRIPTWRITER"/>
      <xs:enumeration value="COMPOSER"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="segmentsType">
    <xs:sequence>
      <xs:element type="segmentType" name="segment" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="segmentType">
    <xs:complexContent>
      <xs:extension base="baseMediaType">
        <xs:sequence>
          <xs:element type="xs:duration" name="start"/>
        </xs:sequence>
        <xs:attribute type="midType" name="midRef" use="required"/>
        <xs:attribute type="xs:anyURI" name="urnRef" use="required"/>
        <xs:attribute type="segmentTypeEnum" name="type" use="required"/>
      </xs:extension>
    </xs:complexContent>

  </xs:complexType>

  <xs:complexType name="relationType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="relationTypeType" name="type" use="required"/>
        <xs:attribute type="xs:string" name="broadcaster" use="required"/>
        <xs:attribute type="xs:anyURI" name="uriRef"/>
        <xs:attribute type="xs:anyURI" name="urn"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="imagesType">
    <xs:sequence>
      <xs:element ref="shared:image" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="groupTableType">
    <xs:sequence>
      <xs:element ref="group" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="websiteType">
    <xs:restriction base="xs:anyURI">
      <xs:minLength value="1"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="twitterType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="ACCOUNT"/>
              <xs:enumeration value="HASHTAG"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="locationTableType">
    <xs:sequence>
      <xs:element type="locationType" name="location" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element type="scheduleEventType" name="scheduleEvent" minOccurs="0" maxOccurs="unbounded"
      />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="scheduleType">
    <xs:sequence>
      <xs:element type="scheduleEventType" name="scheduleEvent" maxOccurs="unbounded" minOccurs="1"
      />
    </xs:sequence>
    <xs:attribute type="channelEnum" name="channel"/>
    <xs:attribute type="xs:string" name="net"/>
    <xs:attribute type="xs:date" name="date"/>
    <xs:attribute type="xs:short" name="releaseVersion"/>
  </xs:complexType>

  <xs:complexType name="scheduleEventsType">
    <xs:sequence>
      <xs:element type="scheduleEventType" name="scheduleEvent" minOccurs="0" maxOccurs="unbounded"
      />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="scheduleEventType">
    <xs:sequence>
      <xs:element type="repeatType" name="repeat" minOccurs="0"/>
      <xs:element type="xs:anyURI" name="memberOf" minOccurs="0"/>
      <xs:element type="avAttributesType" name="avAttributes" minOccurs="0"/>
      <xs:element type="xs:string" name="textSubtitles" minOccurs="0"/>
      <xs:element name="textPage" type="xs:string" minOccurs="0"/>
      <xs:element type="xs:date" name="guideDay"/>
      <xs:element type="xs:dateTime" name="start"/>
      <xs:element type="xs:duration" name="offset" minOccurs="0"/>
      <xs:element type="xs:duration" name="duration"/>
      <xs:element type="xs:string" name="poProgID" minOccurs="0"/>
      <xs:element type="xs:string" name="primaryLifestyle" minOccurs="0"/>
      <xs:element type="xs:string" name="secondaryLifestyle" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="imi"/>
    <xs:attribute type="channelEnum" name="channel"/>
    <xs:attribute type="xs:string" name="net"/>
    <xs:attribute type="xs:date" name="guideDay"/>
    <xs:attribute type="midType" name="midRef" use="required"/>
